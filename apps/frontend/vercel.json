{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VITE_ENVIRONMENT": "test"
  },
  "build": {
    "env": {
      "VITE_API_BASE_URL": "/api",
      "VITE_BACKEND_URL": "https://awade-backend-test.onrender.com",
      "VITE_ENVIRONMENT": "test"
    }
  }
}
